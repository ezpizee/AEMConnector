                        {{#if actions.save}}
                            <button type="submit" class="btn btn-primary"><i class="fas fa-save"></i>&nbsp;{{i18n 'LABEL_SAVE'}}</button>{{/if}}
                    </div>
                </div>
            </div>
        </div>
        <div class="card-body text-justify">
            {{#if fieldKeys.name}}
